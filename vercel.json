{
  "version": 2,
  "routes": [
    {
      "src": "/index.css",
      "dest": "/index.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/frontend.js", 
      "dest": "/frontend.js",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 300
    }
  }
}